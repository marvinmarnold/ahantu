<h1><%= t "search.finalize.title" %></h1>

<%= simple_nested_form_for(@search) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= render "searches/when", f: f %>
    <%= render 'searches/rooms', f: f %>

  </div>

  <hr />

  <div class="actions">
    <%= f.button :submit, class: "btn btn-block btn-success btn-large" %>
  </div>
<% end %>
