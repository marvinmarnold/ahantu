<%= simple_nested_form_for(@search) do |f| %>
  <%= f.error_notification %>

  <div class="inputs row-fluid">
  	<%= render 'searches/homepage/keyword', f: f %>
  	<%= render 'searches/homepage/when', f: f %>
  	<%= render 'searches/homepage/rooms', f: f %>
  </div>
  
	<div class="jumbotron">
	    <%= f.button :submit, class: "btn btn-primary btn-lg" %>
	</div>

<% end %>
