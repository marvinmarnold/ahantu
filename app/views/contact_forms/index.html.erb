<h1>Listing contact_forms</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>From</th>
      <th>To</th>
      <th>State</th>
      <th>Body</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_forms.each do |contact_form| %>
      <tr>
        <td><%= contact_form.subject %></td>
        <td><%= contact_form.from %></td>
        <td><%= contact_form.to %></td>
        <td><%= contact_form.state %></td>
        <td><%= contact_form.body %></td>
        <td><%= link_to 'Show', contact_form %></td>
        <td><%= link_to 'Edit', edit_contact_form_path(contact_form) %></td>
        <td><%= link_to 'Destroy', contact_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact form', new_contact_form_path %>
